export PATH="$HOME/bin:$PATH"
alias gitcav="git commit -a --amend -C HEAD"
alias gittm="git branch --set-upstream-to=origin/master"
alias gitls="git show --pretty="" --name-status"
alias gitbr="git branch -v"
alias gitrhh="git reset --hard HEAD"
alias gitcoh="git checkout HEAD --"
alias gitcom="git checkout HEAD^ --"