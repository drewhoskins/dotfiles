source ~/.bash_include
alias cdps="cd ~/stripe/pay-server"
alias cdstr="cd ~/stripe/temporal-ruby"
alias cdctr="cd ~/coinbase/temporal-ruby"
alias cdpc="cd ~/stripe/puppet-config"
